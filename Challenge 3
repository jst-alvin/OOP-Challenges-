public class Challenge3 {
    //using if else
    public static void main(String[] args){
double price = 45.0;

if(price >100){
    System.out.println("That is very expensive.");
}else if(price>50){
    System.out.println("That is affordable");
        }else{
    System.out.println("That is very cheap");
        }

    }
}
